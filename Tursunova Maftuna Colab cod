import numpy as np  
 
# 1. Massiv yaratish  
 
olma = np.array([1,2,3,4,12])  
uzum = np.array([[1,3,4],[5,6,9]]) 
 
#2. Matematik operatsiyalar 
sum_array=np.sum(olma) 
mean_array=np.mean(olma) 
product_array=np.prod(olma) 
 
sum_array=np.sum(uzum) 
mean_array==np.mean(uzum) 
product_array=np.prod(uzum) 
 
print("olma", olma) 
print("uzum", uzum) 
print("Massivlar yig'indisi:", sum_array) 
print("O'rtacha:", mean_array) 
print("ko'paytma:", product_array) 
 